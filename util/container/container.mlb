ann
   "warnUnused true"
   "forceUsed"
   "nonexhaustiveMatch warn"
   "redundantMatch warn"
   "sequenceNonUnit warn"
in
   local
      $(SML_LIB)/basis/basis.mlb
      ../basic/basic.mlb

      dynamic-array.sig
      dynamic-array.sml
      id-bucket.sig
      id-bucket.sml
      stack.sig
      stack.sml
      queue.sig
      queue.sml
      tree.sig
      tree.fun
      ring.sig
      ring.sml
   in
      signature DYNAMIC_ARRAY
      signature ID_BUCKET
      signature IMPERATIVE_QUEUE
      signature QUEUE
      signature RAM_STACK
      signature RING
      signature STACK
      signature TREE

      structure DynamicArray
      structure IdBucket
      structure ImperativeQueue
      structure Queue
      structure Ring
      structure Stack

      functor Tree
   end
end
